<script>
    var country_name = {
        'Thailand': 'ไทย',
        'Cambodia': 'กัมพูชา'
    };
    var region_name = {
        'Bangkok': 'กรุงเทพมหานคร'
    }

    function get_browser2() {
        var ua = navigator.userAgent,
            tem, M = ua.match(/(opera|chrome|safari|firefox|msie|trident(?=\/))\/?\s*(\d+)/i) || [];
        if (/trident/i.test(M[1])) {
            tem = /\brv[ :]+(\d+)/g.exec(ua) || [];
            return {
                name: 'IE',
                version: (tem[1] || '')
            };
        }
        if (M[1] === 'Chrome') {
            tem = ua.match(/\bOPR|Edge\/(\d+)/)
            if (tem != null) {
                return {
                    name: 'Opera',
                    version: tem[1]
                };
            }
        }
        M = M[2] ? [M[1], M[2]] : [navigator.appName, navigator.appVersion, '-?'];
        if ((tem = ua.match(/version\/(\d+)/i)) != null) {
            M.splice(1, 1, tem[1]);
        }
        return {
            name: M[0],
            version: M[1]
        };

    };

    $.ajax('https://freegeoip.net/json/', {
        type: "get",
        success: function(data) {
            //alert(JSON.stringify(data));
            //data.ip, data.country_code, data.country_name, data.region_name, 
            //alert('a');
            var browser = get_browser2();
            //alert(browser.name+browser.version);
            var access_tpl = 'คุณกำลังเข้าใช้งานระบบบริหารจัดการทรัพยากร​ ICT ด้วย ';
            access_tpl += browser.name + '/' + browser.version;
            access_tpl += ', IP Address ' + data.ip;
            access_tpl += ' ประเทศ' + country_name[data.country_name] + ', ' + region_name[data.region_name];
            $('.copyright').html(access_tpl);
        },
        error: function(fa) {
            //alert(JSON.stringify(fa));
            //alert('b');
        }
    });
</script>