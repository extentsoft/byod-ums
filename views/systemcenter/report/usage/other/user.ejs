        <script>
            account_now = '<%= email %>';
			var start_ ='';
			var end_ ='';
			var data_ = '0';
			//alert(account_now);

            function refresh_table(sd, ed, data) {
                //alert('refresh');
                //alert(data);
                document.getElementById("tab-body").innerHTML = '<div class="text-right"><h1><span class="fa fa-print" id="printer" onclick="printpdf()"></h1></div><table class="table table-striped" id="data-tables"><thead><tr><th>ชื่อผู้ใช้งาน</th><th>จำนวนครั้งที่เข้าใช้งาน</th></tr></thead><tbody></tbody></table>';

                $.ajax('http://byod.excise.go.th/report/report681', {
                    type: "get",
                    data: {
                        start: sd,
                        end: ed,
                        usrname: data
                    },
                    success: function(json) {
                        //alert(JSON.stringify(json));
                        data_res = JSON.parse(JSON.stringify(json));
                        bindata_table(data_res);
                    }
                });
            }


            function bindata_table(data_sql) {
                //alert('bind');
                //alert(data_sql);
                data = [];
                total = 0;
                for (i = 0; i < data_sql.length; i++) {

                    $('#data-tables').find('tbody').append("<tr><td>" + data_sql[0] + "</td><td>" + data_sql[1] + "</td></tr>");
                    //alert(JSON.stringify(data));
                    data_sql.splice(0, 2);
                    i = -1;

                }
                //alert(JSON.stringify(data));
                //c1(data);

                $('#data-tables').dataTable();

            }

            function datepick() {
				moment.locale('th');
                function e(e, a, r) {
					var input, filter, ul, li, a, i;
					input = document.getElementById("myInput");
					document.getElementById("tab-body").innerHTML = '<div class="text-right"><h1><span class="fa fa-print" id="printer" onclick="printpdf()"></h1></div><table class="table table-striped" id="data-tables"><thead><tr><th>ชื่อผู้ใช้งาน</th><th>จำนวนครั้งที่เข้าใช้งาน</th></tr></thead><tbody></tbody></table>';

					filter = input.value.toUpperCase();
					data_ = filter;
					//alert(data_);
					
					if (data_ == '') {
						data_ = '0';
						$('#data-tables').dataTable();
					} else {
						refresh_table(e.format("Y-M-D"), a.format("Y-M-D"),data_);
					}
					
                    
					start_ = e.format("Y-M-D");
					end_ = a.format("Y-M-D");
                    //alert(e.format("Y-M-D"));
                    //alert(a.format("Y-M-D"));


                    var o = "",
                        l = "";
                    a - e < 100 ? (o = "วันนี้:", l = e.format("MMM D")) : "เมื่อวาน" == r ? (o = "เมื่อวาน:", l = e.format("MMM D")) : l = e.format("MMM D") + " - " + a.format("MMM D"),
                        t.find(".m-subheader__daterange-date").html(l),
                        t.find(".m-subheader__daterange-title").html(o)
                }
                if (0 != $("#daterangepicker").length) {
                    var t = $("#daterangepicker"),
                        a = moment(),
                        r = moment();
                    t.daterangepicker({
                            startDate: a,
                            endDate: r,
                            opens: "left",
							showCustomRangeLabel: false,
							alwaysShowCalendars: true,
                            ranges: {
                                "รายงานของวันนี้": [moment(), moment()],
                                //Yesterday: [moment().subtract(1, "days"), moment().subtract(1, "days")],
                                "รายงาน 7 วันล่าสุด": [moment().subtract(6, "days"), moment()],
                                "รายงาน 30 วันล่าสุด": [moment().subtract(29, "days"), moment()],
                                "รายงานของเดือนนี้": [moment().startOf("month"), moment().endOf("month")],
                                "รายงานของปีนี้": [moment().startOf("year"), moment().endOf("year")]
                                    //"Last Month": [moment().subtract(1, "month").startOf("month"), moment().subtract(1, "month").endOf("month")]
                            }
                        }, e),
                        e(a, r, "")
                }
            }
			
			function myFunction() {
                
                var input, filter, ul, li, a, i;
                input = document.getElementById("myInput");
                //document.getElementById("tab-body").innerHTML="";
                document.getElementById("tab-body").innerHTML = '<div class="text-right"><h1><span class="fa fa-print" id="printer" onclick="printpdf()"></h1></div><table class="table table-striped" id="data-tables"><thead><tr><th>ชื่อผู้ใช้งาน</th><th>จำนวนครั้งที่เข้าใช้งาน</th></tr></thead><tbody></tbody></table>';
                //$('#data-tables').find('tbody').empty();

                filter = input.value.toUpperCase();
				data_ = filter;
                //alert(filter);
				if (filter == '') {
					data_ = '0';
                    $('#data-tables').dataTable();
                } else {
                    refresh_table(start_,end_,filter);
                }
                

            }
			
			function printpdf(){
				var search_ = '';
				var date_ = moment().format("DD MMM YYYY");
				if(document.getElementById("table-search").value.trim() == ''){
					search_ = 0;
				}
				else {
					search_ = document.getElementById("table-search").value.trim();					
				}
				location.href = 'http://byod.excise.go.th/api/v1/pdf/2681/'+start_+'/'+end_+'/'+data_+'/'+'0'+'/'+account_now+'/'+search_+'/'+date_;
			}

           
            datepick();
			
        </script>

